[target.x86_64-unknown-linux-gnu]
rustflags = [
    "-C", "target-cpu=x86-64-v2"]

[target.x86_64-unknown-linux-musl]
rustflags = [
    "-C", "target-cpu=x86-64-v2",
    "-C", "target-feature=+crt-static"
]
[target.x86_64-pc-windows-msvc]
rustflags = [
    "-C", "target-cpu=x86-64-v2",
    "-C", "target-feature=+crt-static"
]

[target.x86_64-pc-windows-gnu]
rustflags = [
    "-C", "target-cpu=x86-64-v2",
    "-C", "target-feature=+crt-static"
]
